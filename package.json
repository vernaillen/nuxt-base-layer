{
  "name": "@vernaillen/nuxt-base-layer",
  "type": "module",
  "version": "0.2.18",
  "main": "./nuxt.config.ts",
  "typings": "./index.d.ts",
  "scripts": {
    "dev": "nuxi prepare .playground && nuxi dev .playground",
    "build": "nuxi prepare && nuxi prepare .playground && nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx nuxi@latest prepare",
    "release": "pnpm run build && changelogen --release && npm publish --access public && git push --follow-tags"
  },
  "dependencies": {
    "@nuxt/image": "1.0.0-rc.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/nuxt": "^10.3.0",
    "nuxt-time": "^0.1.1",
    "vue3-lazyload": "^0.3.8"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.8.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "changelogen": "^0.5.4",
    "eslint": "^8.47.0",
    "nuxt": "^3.6.5",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@8.6.10"
}