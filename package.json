{
  "name": "@vernaillen/nuxt-base-layer",
  "type": "module",
  "version": "0.2.25",
  "main": "./nuxt.config.ts",
  "typings": "./index.d.ts",
  "scripts": {
    "dev": "nuxi prepare .playground && nuxi dev .playground",
    "build": "nuxi prepare && nuxi prepare .playground && nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx nuxi@latest prepare",
    "release": "pnpm run build && changelogen --release && npm publish --access public && git push --follow-tags"
  },
  "dependencies": {
    "@nuxt/image": "1.3.0",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "nuxt-time": "^0.1.2",
    "vue3-lazyload": "^0.3.8"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.0.8",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.10.1",
    "nuxt-icon": "^0.6.8",
    "typescript": "^5.3.3"
  }
}